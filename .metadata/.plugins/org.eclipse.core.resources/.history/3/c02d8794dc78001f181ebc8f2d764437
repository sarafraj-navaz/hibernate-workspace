package array_all_Program;

import java.util.Arrays;

public class Test30 {
	public static void main(String[] args) {
		int[] a= {7,0,2,6,0,4};	
		System.out.println(Arrays.toString(afterShiftedArray(a)));
		
	}
	public static int[] afterShiftedArray(int[] a) {
		int n=a.length-1;
		for(int i=0;i<=n;i++) {
			if(a[i]==0) {
				for(int j=i;j<n;j++) {
					a[j]=a[j+1];
				}	
			}
		a[a.length-1]=0;	
		}
	return a;	
	}
}
