package com.jsp.hibernate_one_to_one.dto;

import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.OneToOne;

import lombok.Data;

@Entity
@Data
public class Person {

	@Id
	private int id;
	private String name;
	private String email;
	private long phone;
	

	private  Adhar adhar;
	
	
}
